{
    "name": "selenium-mocha-digital-skola",
    "version": "1.0.0",
    "description": "Selenium WebDriver tests using Mocha with Mochawesome reports",
    "scripts": {
        "test-login": "mocha tests/test-login.js --recursive --timeout 60000",
        "unit-test-login": "mocha tests/unit-test-login.js --recursive --timeout 60000",
        "report-unit-test": "mocha tests/unit-test-login.js --recursive --timeout 60000 --reporter mochawesome",
        "integration-test-login": "mocha tests/integration-test-login.js --recursive --timeout 60000",
        "report-integration-test": "mocha tests/integration-test-login.js --recursive --timeout 60000 --reporter mochawesome",
        "system-test-login": "mocha tests/system-test-login.js --recursive --timeout 60000",
        "report-system-test": "mocha tests/system-test-login.js --recursive --timeout 60000 --reporter mochawesome"
        
    },
    "dependencies": {
        "pixelmatch": "^7.1.0",
        "pngjs": "^7.0.0",
        "selenium-webdriver": "^4.17.0"
    },
    "devDependencies": {
        "mocha": "^10.4.0",
        "mocha-simple-html-reporter": "^2.0.0",
        "mochawesome": "^7.1.3"
    }
}
